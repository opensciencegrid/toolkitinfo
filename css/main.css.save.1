.navbar {
  overflow: hidden;
  background-color: #DBE2EF;
  font-family: Arial, Helvetica, sans-serif;
  padding-top: 16px;
  padding-bottom: 16px;
}

.navbar a {
  float: none;
  font-size: 16px;
  color: white;
  text-align: center;
  padding: 14px 16px;
  text-decoration: none;
}

.block {
  display: block;
  width: 100%;
  border: 2px solid #3F72AF;
  background-color: #DBE2EF;
  padding: 14px 28px;
  font-size: 16px;
  cursor: pointer;
  text-align: center;
  color: #112D4E;
}

.row
{
display: flex;
justify-content: space-evenly;
flex-wrap: wrap; /* added - this tells Flex to wrap onto a new line, if there is no space. */
background-color: #F9F7F7;
}

.column
{
display: inline-block;
padding: 0.5em;
display: block;
width: 33.333%;
box-sizing: border-box; /* added - this change the way padding is applied, so it always stays at 33.33%, regardless of padding */
}

.colform 
{
float:left;
width:50%;
}

.central
{
left: 50%;
position: fixed;
}

.dynamics {
  white-space: nowrap;
}

.dynamics > div {
  width: 300px;
  background: black;
  border: 3px solid white;
  height: 20px;
  display: inline-block;
